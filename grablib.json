{
  "download_root": "libs",
  "sites":
  {
    "github": "https://raw.githubusercontent.com",
    "bootstrap": "{{ github }}/twbs/bootstrap/v3.3.5/dist",
    "font_awesome": "{{ github }}/FortAwesome/Font-Awesome/v4.5.0",
    "video_js": "http://vjs.zencdn.net/5.0"
  },
  "libs": {
    "http://code.jquery.com/jquery-2.1.4.min.js": "js/jquery.min.js",
    "http://gsgd.co.uk/sandbox/jquery/easing/jquery.easing.1.3.min.js": "js/jquery.easing.min.js",

    "{{ bootstrap }}/css/bootstrap.min.css": "css/{{ filename }}",
